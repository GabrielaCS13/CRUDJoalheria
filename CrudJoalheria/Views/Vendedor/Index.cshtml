@model IEnumerable<VendedorModel>

<table class="table table-striped table-dark">
<thead>
<tr>

<th scope="col">Nome</th>
<th scope="col">CPF</th>
<th scope="col">Telefone</th>
<th scope="col">Cidade</th>
</tr>
</thead>
<tbody>
		@if(Model!=null ){
			@foreach (var vendedor in Model)
			{

				<tr>


					<th scope="row">@vendedor.Nome</th>
					<td>@vendedor.CPF</td>
					<td>@vendedor.Telefone</td>
					<td>@vendedor.Cidade/td>
				</tr>
			}
		}
			</tbody>
</table>